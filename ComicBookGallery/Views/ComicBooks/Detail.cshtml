
@{


}
<html>
<head>
    <title></title>
</head>
<body>
    <div>
        <h1>@ViewBag.SeriesTitle</h1>
        <h2>Issue #@ViewBag.IssueNumber</h2>

        <h5>Description:</h5>
        <div>@Html.Raw(ViewBag.Description)</div>
        @if (ViewBag.Artists.Length > 1)
    {
        <h5> Artists:</h5>

        <ul>
        @foreach (var t in ViewBag.Artists)
            { 
                <li>@t</li>
            }
        </ul>
    }
</div>
</body>
</html>